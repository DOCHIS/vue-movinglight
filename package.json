{
  "name": "vue-movinglight",
  "version": "1.0.7",
  "description": "A customizable moving light component for Vue.js applications, perfect for creating dynamic lighting effects, stage lighting simulations, or interactive UI elements.",
  "main": "dist/vue-movinglight.umd.js",
  "module": "dist/vue-movinglight.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "cross-env BUILD_MODE=lib vite build",
    "build:demo": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DOCHIS/vue-movinglight.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "component",
    "light",
    "animation",
    "effect",
    "moving-light",
    "ui"
  ],
  "author": "DOCHIS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DOCHIS/vue-movinglight/issues"
  },
  "homepage": "https://vue-movinglight.vercel.app/",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vue/test-utils": "^2.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^26.0.0",
    "sass": "^1.83.4",
    "terser": "^5.37.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^1.0.0",
    "vue": "^3.3.0",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module"
}
